{
  "name": "<%=appNameSlug%>",
  "description": "<%=appDescription%>",
  "version": "0.1.0",
  "homepage": "https://github.com/trainiac/<%=appName%>",
  "author": {
    "name": "Adrian William Adkiosn",
    "email": "adkison.adrian@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trainiac/<%=appName%>.git"
  },
  "bugs": {
    "url": "https://github.com/trainiac/<%=appName%>/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.prod.config.js",
    "build-server": "babel -d ./build/server ./server -s",
    "clean": "rm -rf build && mkdir build",
    "develop": "node_modules/.bin/babel-node --debug server/app.dev.js",
    "lint": "node_modules/.bin/eslint ./ --max-warnings 0",
    "lint-fix": "node_modules/.bin/eslint --fix ./",
    "prod": "npm run build && npm run start",
    "start": "node ./build/server/app.prod.js",
    "test": "node_modules/.bin/jest --config jest.config.json",
    "test-cov": "node_modules/.bin/jest --config jest.coverage.config.json"
  },
  "keywords": [
    "react",
    "redux",
    "express"
  ],
  "license": "MIT",
  "engines": {
    "node": "5.1.0"
  },
  "dependencies": {
    "aphrodite": "0.6.0",
    "babel-polyfill": "6.16.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "express": "4.14.0",
    "express-jsonschema": "1.1.6",
    "http-status-codes": "1.0.6",
    "isomorphic-fetch": "2.1.1",
    "jquery": "3.1.1",
    "lodash": "4.16.4",
    "morgan": "1.7.0",
    "node-fetch": "1.6.3",
    "normalizr": "2.2.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-logger": "2.7.0",
    "redux-thunk": "2.1.0",
    "serve-favicon": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-react": "6.16.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "eslint": "3.9.1",
    "eslint-plugin-react": "6.5.0",
    "extract-text-webpack-plugin": "1.0.1",
    "react-hot-loader": "3.0.0-beta.5",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.7.0",
    "jest": "16.0.2",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0"
  }
}
